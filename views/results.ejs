<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/css/styles.css">
  <title>Results</title>
</head>
<body>
  <h1>Results</h1>
  <p><%= message %></p>

  <% if (commonLikes.length > 0) { %>
    <h3>Common Likes:</h3>
    <ul>
      <% commonLikes.forEach(like => { %>
        <li><%= like %></li>
      <% }) %>
    </ul>
  <% } %>

  <p>Compatibility: <%= compatibility %> %</p>

  <a href="/user/main">Go Back</a>
</body>
</html>

